@model IEnumerable<D.Models.CustomerEnt>


@using D.Models


        @foreach (CustomerEnt p in Model)
        {
            <tr>
                <td class="text-center">
                    <strong>@Html.HiddenFor(modelItem => p.ID_клиента) @Html.DisplayTextFor(modelItem => p.УНП_Клиента)</strong>
                </td>
                <td class="text-center"><strong name="client">@Html.DisplayTextFor(modelItem => p.Название_организации)</strong></td>

            </tr>

        }
    

