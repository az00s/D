@model IEnumerable<D.Models.Клиент>


@using D.Models

<div class="ab">
    <table class="table-bordered table-striped" id="tbClients">
        @foreach (Клиент p in Model)
        {
            <tr>
                <td>
                    <strong>@Html.HiddenFor(modelItem => p.ID_клиента) @Html.DisplayTextFor(modelItem => p.УНП_Клиента)</strong>
                </td>
                <td><strong>@Html.DisplayTextFor(modelItem => p.Название_организации)</strong></td>

            </tr>

        }
    </table>


</div>

